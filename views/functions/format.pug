  function formatDate(date) {
    var d = new Date(date),
        month = '' + (d.getMonth() + 1),
        day = '' + d.getDate(),
        year = d.getFullYear();
    if (Number.isNaN(month) || Number.isNaN(day) || Number.isNaN(year))
      return '';

    if (month.length < 2) 
        month = '0' + month;
    if (day.length < 2) 
        day = '0' + day;

    return [year, month, day].join('-');
  }

  function formatUrl(url) {
    return url.substring(0,5) + "..." + url.substring(url.length - 5);
  }